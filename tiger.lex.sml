                                                      yytext);
                               valid_str := false)
                            else buff_string := !buff_string ^
                                                String.str(chr(tempnum))
                        end;
                        continue() end
| 177 => (YYBEGIN STRING;
                        buff_string := !buff_string ^ "\"";
                        continue())
| 179 => (YYBEGIN STRING;
                        buff_string := !buff_string ^ "\\";
                        continue())
| 183 => let val yytext=yymktext() in YYBEGIN SLASH_M;
                        buff_string := !buff_string ^ yytext;
                        format_flag := true;
                        continue() end
| 185 => (YYBEGIN SLASH_M;
                        buff_string := !buff_string ^ "\n";
                        format_flag := true;
                        lineNum := !lineNum + 1;
                        linePos := yypos :: !linePos;
                        continue())
| 187 => let val yytext=yymktext() in YYBEGIN SLASH_M;
                        buff_string := !buff_string ^ yytext;
                        format_flag := false;
                        continue() end
| 189 => (buff_string := !buff_string ^ "\n";
                        format_flag := true;
                        lineNum := !lineNum + 1;
                        linePos := yypos :: !linePos;
                        continue())
| 193 => let val yytext=yymktext() in buff_string := !buff_string ^ yytext;
                        format_flag := true;
                        continue() end
